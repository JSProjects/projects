<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
version="2.5">

<display-name>Welcome to Tomcat</display-name>
<description>
Welcome to Tomcat
</description>
<servlet>
<servlet-name>searchServlet</servlet-name>
<servlet-class>report.Search</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>searchServlet</servlet-name>
<url-pattern>/search/*</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>soSearchServlet</servlet-name>
<servlet-class>report.SOSearch</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>soSearchServlet</servlet-name>
<url-pattern>/sosearch/*</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>loginServlet</servlet-name>
<servlet-class>report.login</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>loginServlet</servlet-name>
<url-pattern>/login/*</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>ChngPwdServlet</servlet-name>
<servlet-class>report.ChngPwd</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>ChngPwdServlet</servlet-name>
<url-pattern>/chngpwd/*</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>SummaryServlet</servlet-name>
<servlet-class>report.Summary</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>SummaryServlet</servlet-name>
<url-pattern>/summary/*</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>ActiveSummaryServlet</servlet-name>
<servlet-class>report.ActiveSummary</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>ActiveSummaryServlet</servlet-name>
<url-pattern>/activation/*</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>searchDatesServlet</servlet-name>
<servlet-class>report.SearchDates</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>searchDatesServlet</servlet-name>
<url-pattern>/searchdates/*</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>viewSearchServlet</servlet-name>
<servlet-class>report.ViewSearch</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>viewSOSearchServlet</servlet-name>
<url-pattern>/viewSOSearch/*</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>viewSOSearchServlet</servlet-name>
<servlet-class>report.SOSearchExtract</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>viewSearchServlet</servlet-name>
<url-pattern>/viewSearch/*</url-pattern>
</servlet-mapping>
<welcome-file-list>
    <welcome-file>login.jsp</welcome-file> 
<!--    <welcome-file>index.jsp</welcome-file> -->
 <!--  <welcome-file>test.jsp</welcome-file> -->
 </welcome-file-list>
</web-app>